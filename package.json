    },
    "repository": {
        "type": "git",
        "url": "https://github.com/w8r/Leaflet.Shim.git"
    },
    "keywords": [
        "leaflet",
        "map",
        "vector",
        "backdrop",
        "shim"
    ],
    "author": "w8r",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/w8r/Leaflet.Shim/issues"
    },
    "homepage": "https://w8r.github.io/Leaflet.Shim/",
    "devDependencies": {
        "uglify-js": "^2.4.15"
    }
}
